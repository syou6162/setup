#!/bin/bash
set -euxo pipefail

formulas=( \
  docker \
  iterm2 \
  google-chrome \
  slack \
  hammerspoon \
  visual-studio-code \
  1password \
  aquaskk \
  alfred \
  karabiner-elements \
  google-cloud-sdk \
  google-japanese-ime \
)

for formula in "${formulas[@]}"; do
  tools/brew-cask-install "$formula"
done
